{"ast":null,"code":"// @ is an alias to /src\nimport HelloWorld from '@/components/HelloWorld.vue';\nexport default {\n  name: 'HomeView',\n  data() {\n    const item = {\n      username: 'ning',\n      nickname: '战胜',\n      email: 'ning@123',\n      phone: '123456789',\n      address: '上海市普陀区金沙江路 1518 弄'\n    };\n    return {\n      tableData: Array(10).fill(item),\n      total: 100,\n      pageIndex: 1,\n      pageSize: 5,\n      collapseBtnClass: 'el-icon-s-fold',\n      isCollapse: false,\n      sideWidth: 200,\n      logoTextShow: true,\n      dialogFormVisible: false,\n      //新增弹窗是否展示\n      form: {},\n      username: \"\"\n    };\n  },\n  created() {\n    this.loadPage(); //分页加载数据\n  },\n\n  methods: {\n    collapse() {\n      //【点击收缩展开按钮函数】\n      this.isCollapse = !this.isCollapse;\n      if (this.isCollapse) {\n        //收缩\n        this.sideWidth = 64;\n        this.collapseBtnClass = 'el-icon-s-unfold';\n        this.logoTextShow = false;\n      } else {\n        //展开\n        this.sideWidth = 200;\n        this.collapseBtnClass = 'el-icon-s-fold';\n        this.logoTextShow = true;\n      }\n    },\n    loadPage() {\n      // //1. 使用原始的请求后端数据方法\n      // fetch(\"http://localhost:9090/user/page?pageIndex=\"+this.pageIndex+\"&pageSize=\"+this.pageSize).then(res => res.json()).then(res => {\n      //     console.log(res);\n      //     this.tableData = res.data;\n      //     this.total = res.total\n      // });\n\n      // 2.使用 axios 请求后端数据\n      const _this = this;\n      this.request.get(\"http://localhost:9090/user/page\", {\n        params: {\n          pageIndex: _this.pageIndex,\n          pageSize: _this.pageSize,\n          username: _this.username\n        }\n      }).then(res => {\n        console.log(res);\n        _this.tableData = res.records;\n        _this.total = res.total;\n      });\n    },\n    handleSizeChange(pageSize) {\n      console.log(`每页 ${pageSize} 条`);\n      this.pageSize = pageSize;\n      this.loadPage(); //分页加载数据\n    },\n\n    handleCurrentChange(pageIndex) {\n      console.log(`当前页: ${pageIndex}`);\n      this.pageIndex = pageIndex;\n      this.loadPage(); //分页加载数据\n    },\n\n    reset() {\n      //重置按钮\n      this.username = \"\";\n      this.loadPage();\n    }\n  }\n};","map":{"version":3,"names":["HelloWorld","name","data","item","username","nickname","email","phone","address","tableData","Array","fill","total","pageIndex","pageSize","collapseBtnClass","isCollapse","sideWidth","logoTextShow","dialogFormVisible","form","created","loadPage","methods","collapse","_this","request","get","params","then","res","console","log","records","handleSizeChange","handleCurrentChange","reset"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n  <el-container style=\"min-height:100vh; border: 1px solid #eee\">\n\n    <!--左侧导航-->\n    <el-aside :width=\"sideWidth+'px'\" style=\"background-color: rgb(238, 241, 246);height: 100%;\">\n      <el-menu :default-openeds=\"['1', '3']\" style=\"min-height: 100vh; overflow-x: hidden;\"\n               background-color=\"rgb(48, 65, 86)\"\n               text-color=\"#fff\"\n               active-text-color=\"#ffd04b\"\n               :collapse-transition=\"false\"\n               :collapse=\"isCollapse\"\n      >\n        <div style=\"height: 60px;line-height: 60px;text-align: center;\">\n          <img src=\"../assets/logo.png\" alt=\"\" style=\"width: 20px;position: relative;top:5px;margin-right: 5px\"/>\n          <b style=\"color: #fff\" v-show=\"logoTextShow\">后台管理系统</b>\n        </div>\n        <el-submenu index=\"1\">\n          <template slot=\"title\"><i class=\"el-icon-message\"></i>\n            <span slot=\"title\">导航一</span>\n          </template>\n          <el-menu-item-group>\n            <template slot=\"title\">分组一</template>\n            <el-menu-item index=\"1-1\">选项1</el-menu-item>\n          </el-menu-item-group>\n          <el-menu-item-group title=\"分组2\">\n            <el-menu-item index=\"1-3\">选项3</el-menu-item>\n          </el-menu-item-group>\n          <el-submenu index=\"1-4\">\n            <template slot=\"title\">选项4</template>\n            <el-menu-item index=\"1-4-1\">选项4-1</el-menu-item>\n          </el-submenu>\n        </el-submenu>\n        <el-submenu index=\"2\">\n          <template slot=\"title\"><i class=\"el-icon-menu\"></i>\n            <span slot=\"title\">导航二</span>\n          </template>\n          <el-menu-item-group>\n            <template slot=\"title\">分组一</template>\n            <el-menu-item index=\"2-1\">选项1</el-menu-item>\n            <el-menu-item index=\"2-2\">选项2</el-menu-item>\n          </el-menu-item-group>\n        </el-submenu>\n        <el-submenu index=\"3\">\n          <template slot=\"title\"><i class=\"el-icon-setting\"></i>\n            <span slot=\"title\">导航三</span>\n          </template>\n          <el-menu-item-group>\n            <template slot=\"title\">分组一</template>\n            <el-menu-item index=\"3-1\">选项1</el-menu-item>\n            <el-menu-item index=\"3-2\">选项2</el-menu-item>\n          </el-menu-item-group>\n        </el-submenu>\n      </el-menu>\n    </el-aside>\n\n    <!--右边主体内容-->\n    <el-container>\n      <el-header style=\"font-size: 12px ; border-bottom: 1px solid #ccc; line-height: 60px;display: flex;\">\n        <div style=\"flex: 1;font-size: 20px;\">\n          <span :class=\"collapseBtnClass\" style=\"margin-right: 15px\" @click=\"collapse\"></span>\n        </div>\n        <el-dropdown style=\"width: 70px;cursor: pointer;\">\n          <span>王小虎</span><i class=\"el-icon-arrow-down\" style=\"margin-left: 5px\"></i>\n          <el-dropdown-menu slot=\"dropdown\">\n            <el-dropdown-item>个人信息</el-dropdown-item>\n            <el-dropdown-item>退出</el-dropdown-item>\n          </el-dropdown-menu>\n        </el-dropdown>\n\n      </el-header>\n\n      <el-main>\n        <!--Breadcrumb 页屑-->\n        <div style=\"margin-bottom: 30px\">\n          <el-breadcrumb separator=\"/\">\n            <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\n            <el-breadcrumb-item><a href=\"/\">用户管理</a></el-breadcrumb-item>\n          </el-breadcrumb>\n        </div>\n\n        <!--搜索框-->\n        <div style=\"padding: 10px 0;\">\n          <el-input style=\"width: 200px;\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"username\" ></el-input>\n          <!--<el-input style=\"width: 200px;\" placeholder=\"请输入邮箱\" suffix-icon=\"el-icon-message\" class=\"ml-5\"></el-input>-->\n          <!--<el-input style=\"width: 200px;\" placeholder=\"请输入地址\" suffix-icon=\"el-icon-position\"  class=\"ml-5\"></el-input>-->\n          <el-button class=\"ml-5\" type=\"primary\" @click=\"loadPage\">搜索</el-button>\n          <el-button  type=\"warning\" @click=\"reset\">重置</el-button>\n        </div>\n        <!--新增-->\n        <div style=\"margin: 10px 0\">\n          <el-button type=\"primary\">新增 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\n          <el-button type=\"danger\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\n          <el-button type=\"primary\">导入 <i class=\"el-icon-download\"></i></el-button>\n          <el-button type=\"primary\">导出 <i class=\"el-icon-upload2\"></i></el-button>\n        </div>\n\n        <el-table :data=\"tableData\" border stripe header-cell-class-name=\"tableHeaderBg\">\n          <el-table-column prop=\"id\" label=\"编号\" width=\"60\"></el-table-column>\n          <el-table-column prop=\"username\" label=\"用户名\" width=\"120\"></el-table-column>\n          <el-table-column prop=\"nickname\" label=\"昵称\"></el-table-column>\n          <el-table-column prop=\"email\" label=\"邮箱\"></el-table-column>\n          <el-table-column prop=\"phone\" label=\"电话\"></el-table-column>\n          <el-table-column prop=\"address\" label=\"地址\"></el-table-column>\n          <el-table-column prop=\"op\" label=\"操作\">\n            <template>\n              <el-button type=\"success\">编辑 <i class=\"el-icon-edit-outline\"></i></el-button>\n              <el-button type=\"danger\">删除 <i class=\"el-icon-delete\"></i></el-button>\n            </template>\n          </el-table-column>\n        </el-table>\n\n        <!--分页-->\n        <div style=\"padding: 10px 0;\">\n          <el-pagination\n                  @size-change=\"handleSizeChange\"\n                  @current-change=\"handleCurrentChange\"\n                  :page-sizes=\"[5, 10, 15, 20]\"\n                  :current-page=\"pageIndex\"\n                  :page-size=\"pageSize\"\n                  layout=\"total, sizes, prev, pager, next, jumper\"\n                  :total=\"total\">\n          </el-pagination>\n        </div>\n\n\n        <!--弹窗-->\n        <el-dialog title=\"用户信息\" :visible.sync=\"dialogFormVisible\">\n          <el-form label-width=\"120\">\n            <el-form-item label=\"用户名\"><el-input v-model=\"form.name\" autocomplete=\"off\"></el-input></el-form-item>\n            <el-form-item label=\"昵称\"><el-input v-model=\"form.nickname\" autocomplete=\"off\"></el-input></el-form-item>\n            <el-form-item label=\"邮箱\"><el-input v-model=\"form.email\" autocomplete=\"off\"></el-input></el-form-item>\n            <el-form-item label=\"电话\"><el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input></el-form-item>\n            <el-form-item label=\"地址\"><el-input v-model=\"form.address\" autocomplete=\"off\"></el-input></el-form-item>\n          </el-form>\n          <div slot=\"footer\" class=\"dialog-footer\">\n            <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"dialogFormVisible = false\">确 定</el-button>\n          </div>\n        </el-dialog>\n\n\n      </el-main>\n    </el-container>\n\n\n  </el-container>\n</template>\n\n\n\n<script>\n// @ is an alias to /src\nimport HelloWorld from '@/components/HelloWorld.vue'\nexport default {\n  name: 'HomeView',\n  data(){\n      const item = {\n          username: 'ning',\n          nickname: '战胜',\n          email: 'ning@123',\n          phone: '123456789',\n          address: '上海市普陀区金沙江路 1518 弄'\n      };\n      return {\n          tableData: Array(10).fill(item),\n          total: 100,\n          pageIndex: 1,\n          pageSize: 5,\n          collapseBtnClass:'el-icon-s-fold',\n          isCollapse:false,\n          sideWidth:200,\n          logoTextShow:true,\n          dialogFormVisible: false, //新增弹窗是否展示\n          form: {},\n\n          username: \"\",\n      }\n  },\n  created(){\n      this.loadPage();//分页加载数据\n  },\n  methods:{\n      collapse(){//【点击收缩展开按钮函数】\n          this.isCollapse = !this.isCollapse;\n          if(this.isCollapse){//收缩\n              this.sideWidth = 64;\n              this.collapseBtnClass = 'el-icon-s-unfold';\n              this.logoTextShow = false;\n          } else{//展开\n              this.sideWidth = 200;\n              this.collapseBtnClass = 'el-icon-s-fold';\n              this.logoTextShow = true;\n          }\n      },\n      loadPage(){\n          // //1. 使用原始的请求后端数据方法\n          // fetch(\"http://localhost:9090/user/page?pageIndex=\"+this.pageIndex+\"&pageSize=\"+this.pageSize).then(res => res.json()).then(res => {\n          //     console.log(res);\n          //     this.tableData = res.data;\n          //     this.total = res.total\n          // });\n\n          // 2.使用 axios 请求后端数据\n          const _this = this;\n          this.request.get(\"http://localhost:9090/user/page\",{\n              params: {\n                  pageIndex: _this.pageIndex,\n                  pageSize: _this.pageSize,\n                  username: _this.username\n              }\n          }).then(res => {\n              console.log(res);\n              _this.tableData = res.records;\n              _this.total = res.total;\n          });\n      },\n      handleSizeChange(pageSize){\n          console.log(`每页 ${pageSize} 条`);\n          this.pageSize = pageSize;\n          this.loadPage();//分页加载数据\n      },\n      handleCurrentChange(pageIndex){\n          console.log(`当前页: ${pageIndex}`);\n          this.pageIndex = pageIndex;\n          this.loadPage();//分页加载数据\n      },\n      reset(){//重置按钮\n          this.username = \"\";\n          this.loadPage();\n      }\n  }\n}\n</script>\n\n<style>\n  .tableHeaderBg{\n    background: #eee!important;\n  }\n</style>\n"],"mappings":"AAuJA;AACA,OAAAA,UAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA,MAAAC,IAAA;MACAC,QAAA;MACAC,QAAA;MACAC,KAAA;MACAC,KAAA;MACAC,OAAA;IACA;IACA;MACAC,SAAA,EAAAC,KAAA,KAAAC,IAAA,CAAAR,IAAA;MACAS,KAAA;MACAC,SAAA;MACAC,QAAA;MACAC,gBAAA;MACAC,UAAA;MACAC,SAAA;MACAC,YAAA;MACAC,iBAAA;MAAA;MACAC,IAAA;MAEAhB,QAAA;IACA;EACA;EACAiB,QAAA;IACA,KAAAC,QAAA;EACA;;EACAC,OAAA;IACAC,SAAA;MAAA;MACA,KAAAR,UAAA,SAAAA,UAAA;MACA,SAAAA,UAAA;QAAA;QACA,KAAAC,SAAA;QACA,KAAAF,gBAAA;QACA,KAAAG,YAAA;MACA;QAAA;QACA,KAAAD,SAAA;QACA,KAAAF,gBAAA;QACA,KAAAG,YAAA;MACA;IACA;IACAI,SAAA;MACA;MACA;MACA;MACA;MACA;MACA;;MAEA;MACA,MAAAG,KAAA;MACA,KAAAC,OAAA,CAAAC,GAAA;QACAC,MAAA;UACAf,SAAA,EAAAY,KAAA,CAAAZ,SAAA;UACAC,QAAA,EAAAW,KAAA,CAAAX,QAAA;UACAV,QAAA,EAAAqB,KAAA,CAAArB;QACA;MACA,GAAAyB,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACAL,KAAA,CAAAhB,SAAA,GAAAqB,GAAA,CAAAG,OAAA;QACAR,KAAA,CAAAb,KAAA,GAAAkB,GAAA,CAAAlB,KAAA;MACA;IACA;IACAsB,iBAAApB,QAAA;MACAiB,OAAA,CAAAC,GAAA,OAAAlB,QAAA;MACA,KAAAA,QAAA,GAAAA,QAAA;MACA,KAAAQ,QAAA;IACA;;IACAa,oBAAAtB,SAAA;MACAkB,OAAA,CAAAC,GAAA,SAAAnB,SAAA;MACA,KAAAA,SAAA,GAAAA,SAAA;MACA,KAAAS,QAAA;IACA;;IACAc,MAAA;MAAA;MACA,KAAAhC,QAAA;MACA,KAAAkB,QAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}